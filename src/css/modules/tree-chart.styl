#tree-container
  .node
    cursor pointer
    circle
      fill #fff
      stroke steelblue
      stroke-width 1.5px
    text
      font 14px sans-serif
    &.active
      circle
        fill #fc0
        stroke #fc0
        stroke-width 3px

  .link
    fill none
    stroke #ccc
    stroke-width 1.5px

