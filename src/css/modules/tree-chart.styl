#tree-container
  .node
    cursor pointer
    user-select none
    circle
      fill #ccc
      stroke #ccc
      stroke-width 0
    text
      font 14px sans-serif
      fill #ccc
    &.active
      circle
        fill #fff
        stroke #000
        stroke-width 2px
      text
        fill #000

  .link
    fill none
    stroke #ccc
    stroke-width 1.5px

